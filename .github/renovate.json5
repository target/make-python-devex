{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  // This inhibits Renovate automerging updates when Dependabot handles that on public GitHub.
  extends: [
    ":preserveSemverRanges",
  ],
  packageRules: [
    {
      automerge: false,
      matchPackageNames: [
        '*',
      ],
    },
  ],
}
